	pages = {248--52}
}

@article{harris_young_2011,
	title = {Young children's selective trust in informants.},
	volume = {366},
	issn = {1471-2970},
	doi = {10.1098/rstb.2010.0321},
	abstract = {Young children readily act on information from adults, setting aside their own prior convictions and even continuing to trust informants who make claims that are manifestly false. Such credulity is consistent with a long-standing philosophical and scientific conception of young children as prone to indiscriminate trust. Against this conception, we argue that children trust some informants more than others. In particular, they use two major heuristics. First, they keep track of the history of potential informants. Faced with conflicting claims, they endorse claims made by someone who has provided reliable care or reliable information in the past. Second, they monitor the cultural standing of potential informants. Faced with conflicting claims, children endorse claims made by someone who belongs to a consensus and whose behaviour abides by, rather than deviating from, the norms of their group. The first heuristic is likely to promote receptivity to information offered by familiar caregivers, whereas the second heuristic is likely to promote a broader receptivity to informants from the same culture},
	number = {1567},
	journal = {Philos Trans R Soc Lond B Biol Sci},
	author = {Harris, Paul L. and Corriveau, Kathleen H.},
	year = {2011},
	pmcid = {PMC3049091},
	keywords = {Adult, Age Factors, Child, Child Development, Discrimination (Psychology), Humans, Leadership - general, Learning, MA\_leadership, Preschool, Trust},
