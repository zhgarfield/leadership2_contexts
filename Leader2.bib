@incollection{johnson_leadership_2015,
	title = {Leadership in {War}: {Evolution}, {Cognition}, and the {Military} {Intelligence} {Hypothesis}},
	copyright = {Copyright © 2016 John Wiley \& Sons, Inc. All rights reserved.},
	isbn = {978-1-119-12556-3},
	shorttitle = {Leadership in {War}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9781119125563.evpsych229},
	abstract = {War is the mother of all collective action problems: Only a groupcan effectively attack or defend against another, but in doing so each individual risks injury or death. If a group is to survive or defeat organized enemies at reasonable cost, effective leadership is vital to strategize, prepare, and coordinate the activity of multiple individuals. The first section explores the evolutionary origins of leadership behavior in intergroup conflict among: (a) nonhuman animals, (b) primates, and (c) preindustrial societies. While there are precedents for leadership and for war in other animals, explicit leadership in combat appears to be uniquely human. The second section explores the legacy of human evolution for contemporary war leadership, outlining: (a) differences between prehistoric, ancient, and modern warfare; (b) the impact of evolved judgment and decision-making biases on modern war (e.g., in-group/out-group bias and overconfidence); and (c) whether humans might have evolved explicit traits for war leadership. The third section proposes the “Military Intelligence Hypothesis”: War is a deadly game of decisionmaking under uncertainty, and I suggest the significant cognitive demands of (and selection pressure on) effective strategic thinking may have contributed to the evolution of human intelligence. War may have made humans as much as humans made war.},
	language = {en},
	urldate = {2018-10-18},
	booktitle = {The {Handbook} of {Evolutionary} {Psychology}},
	publisher = {American Cancer Society},
	author = {Johnson, Dominic D. P.},
	year = {2015},
	doi = {10.1002/9781119125563.evpsych229},
	keywords = {leadership, evolution, coordination, psychology, war},
	pages = {1--23},
	file = {Snapshot:/Users/zgarf/Zotero/storage/HRDXACPV/9781119125563.html:text/html}
}

