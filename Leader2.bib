	keywords = {Ev Psych, Jepson, Leadership, Leadership - general, MA\_leadership, NSF1, prelims, Prestige / Dominance},
	pages = {1--17},
	file = {The%20evolution%20of%20leader-follow:/Users/hagen/Zotero/storage/BSEEKZZF/The evolution of leader-follow.pdf:application/pdf}
