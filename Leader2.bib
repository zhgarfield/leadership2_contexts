@misc{french_social_2018,
	title = {Social {Monogamy} in {Nonhuman} {Primates}: {Phylogeny}, {Phenotype}, and {Physiology}.},
	shorttitle = {Social {Monogamy} in {Nonhuman} {Primates}},
	url = {/paper/Social-Monogamy-in-Nonhuman-Primates%3A-Phylogeny%2C-French-Cavanaugh/88475d0ca76f8cc4f9fd5d00180f5140108e2db8},
	abstract = {Monogamy as a social system has been both a scientific puzzle and a sociocultural issue for decades. In this review, we examine social monogamy from a comparative perspective with a focus on primates, our closest genetic relatives. We break down monogamy into component elements, including pair-bonding and partner preference, mate guarding or jealousy, social attachment, and biparental care. Our survey of primates shows that not all features are present in species classified as socially monogamous, in the same way that human monogamous relationships may not include all elements-a perspective we refer to as \&quot;monogamy à la carte.\&quot; Our review includes a survey of the neurobiological correlates of social monogamy in primates, exploring unique or common pathways for the elemental components of monogamy. This compilation reveals that the components of monogamy are modulated by a suite of androgenic steroids, glucocorticoid hormones, the nonapeptide hormones oxytocin and vasopressin, and other neurotransmitter systems (e.g., dopamine and opioids). We propose that efforts to understand the biological underpinnings of complex human and animal sociosexual relationships will be well served by exploring individual phenotypic traits, as opposed to pursuing these questions with the assumption that monogamy is a unitary trait or a species-specific characteristic.},
	urldate = {2018-08-30},
	journal = {undefined},
	author = {French, J. Austin and Cavanaugh, Jon and Mustoe, Aaryn C. and Carp, Sarah B. and Womack, Stephanie L.},
	year = {2018},
	file = {Full Text PDF:/Users/hagen/Zotero/storage/5DRTG28Z/French et al. - 2018 - Social Monogamy in Nonhuman Primates Phylogeny, P.pdf:application/pdf;Snapshot:/Users/hagen/Zotero/storage/3HGMTREB/88475d0ca76f8cc4f9fd5d00180f5140108e2db8.html:text/html}
}

